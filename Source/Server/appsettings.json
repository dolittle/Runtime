{
    "dolittle": {
        "runtime": {
            "endpoints": {
                "public": {
                    "port": 5052
                },
                "private": {
                    "port": 5053
                },
                "management": {
                    "port": 5152
                },
                "managementweb": {
                    "port": 5153
                }
            }
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System": "Information",
            "Proto": "Warning",
            "Microsoft": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Grpc.AspNetCore": "Warning",
            "Dolittle": "Information"
        },
        "Console": {
            "FormatterName": "Simple",
            "FormatterOptions": {
                "IncludeScopes": true,
                "TimestampFormat": "HH:mm:ss ",
                "UseUtcTimestamp": true
            }
        }
    }
}
