{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System": "Information",
            "Microsoft": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Dolittle": "Information"
        },
        "Console": {
            "FormatterName": "Simple",
            "FormatterOptions": {
                "IncludeScopes": true,
                "TimestampFormat": "HH:mm:ss ",
                "UseUtcTimestamp": true
            }
        }
    }
}
